{
  "manifest_version": 2,
  "name": "RSigTool",
  "version": "0.1.0",
  "description": "RChain Signature Tool",
  "permissions": [
    "storage"
  ],
  "content_security_policy": "script-src 'sha256-PeWY5DJQb4NK21rhqfZE25WIevBwgCqYE4NW93M2PLw='; object-src 'self'",
  "icons": {
    "48": "icons/rchain.png"
  },
  "browser_action": {
    "default_icon": "icons/rchain.png",
    "default_title": "RChain Signature Tool",
    "default_popup": "tool.html"
  }
}
